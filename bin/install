#!/bin/bash

LOGFILE="${OPENSHIFT_TMP_DIR}/install.log"

date > $LOGFILE
echo "Running install" >> $LOGFILE

mysql -h $OPENSHIFT_MYSQL_DB_HOST -u $OPENSHIFT_MYSQL_DB_USERNAME -p${OPENSHIFT_MYSQL_DB_PASSWORD} -D seafile3 -e "create table users(user_id int not null auto_increment, username varchar(200), PRIMARY KEY(user_id)); insert into users values (null, 'will'); insert into users values (null, 'john');"

echo "Ran install" >> $LOGFILE
